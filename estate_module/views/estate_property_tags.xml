<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
<!--    form-->
    <record id="property_tags_form" model="ir.ui.view">
        <field name="name">Property Tags Form</field>
        <field name="model">property.tag.tags</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name"/>
                </group>
            </form>
        </field>
    </record>
<!--    tree-->
    <record id="property_tags_tree" model="ir.ui.view">
        <field name="name">Property Tags Tree</field>
        <field name="model">property.tag.tags</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>
<!--    action-->
    <record id="property_actions_tag" model="ir.actions.act_window">
        <field name="name">Property Tags</field>
        <field name="res_model">property.tag.tags</field>
        <field name="view_mode">tree,form</field>
    </record>
<!--menu-->
    <menuitem id="property_tags" name="Property Tags" parent="property_settings" sequence="2" action="property_actions_tag"/>
</odoo>